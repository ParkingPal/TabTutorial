<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".customFragment">

    <TextView
        android:id="@+id/textView7"
        android:layout_width="164dp"
        android:layout_height="44dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        android:background="#80FFEB3B"
        android:gravity="center"
        android:text="CRUST"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textSize="30sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="368dp"
        android:layout_height="44dp"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="8dp"
        android:background="#80FFEB3B"
        android:gravity="center"
        android:text="MEAT"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textSize="30sp"
        app:layout_constraintStart_toEndOf="@+id/textView7"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="368dp"
        android:layout_height="44dp"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="22dp"
        android:layout_marginRight="22dp"
        android:background="#80FFEB3B"
        android:gravity="center"
        android:text="VEGGIE"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textSize="30sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView8"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btnRegular"
        android:layout_width="164dp"
        android:layout_height="114dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        android:background="#D4E4F1"
        android:text="Regular"
        android:textSize="30sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />

    <Button
        android:id="@+id/btnThin"
        android:layout_width="164dp"
        android:layout_height="114dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        android:background="#D4E4F1"
        android:text="Thin"
        android:textSize="30sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnRegular" />

    <Button
        android:id="@+id/btnPan"
        android:layout_width="164dp"
        android:layout_height="114dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        android:background="#D4E4F1"
        android:text="Pan"
        android:textSize="30sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnThin" />

    <Button
        android:id="@+id/btnPep"
        android:layout_width="180dp"
        android:layout_height="84dp"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="8dp"
        android:background="#F3DFDD"
        android:text="Pep"
        android:textSize="30sp"
        app:layout_constraintStart_toEndOf="@+id/btnRegular"
        app:layout_constraintTop_toBottomOf="@+id/textView8" />

    <Button
        android:id="@+id/btnBacon"
        android:layout_width="180dp"
        android:layout_height="84dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        android:background="#F3DFDD"
        android:text="Bacon"
        android:textSize="30sp"
        app:layout_constraintStart_toEndOf="@+id/btnPep"
        app:layout_constraintTop_toBottomOf="@+id/textView8" />

    <Button
        android:id="@+id/btnBeef"
        android:layout_width="180dp"
        android:layout_height="84dp"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:background="#F3DFDD"
        android:text="Beef"
        android:textSize="30sp"
        app:layout_constraintEnd_toStartOf="@+id/btnHam"
        app:layout_constraintStart_toEndOf="@+id/btnRegular"
        app:layout_constraintTop_toBottomOf="@+id/btnPep" />

    <Button
        android:id="@+id/btnHam"
        android:layout_width="180dp"
        android:layout_height="84dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        android:background="#F3DFDD"
        android:text="Ham"
        android:textSize="30sp"
        app:layout_constraintStart_toEndOf="@+id/btnBeef"
        app:layout_constraintTop_toBottomOf="@+id/btnBacon" />

    <Button
        android:id="@+id/btnSaus"
        android:layout_width="180dp"
        android:layout_height="83dp"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:background="#F3DFDD"
        android:text="Saus"
        android:textSize="30sp"
        app:layout_constraintEnd_toStartOf="@+id/btnChkn"
        app:layout_constraintStart_toEndOf="@+id/btnThin"
        app:layout_constraintTop_toBottomOf="@+id/btnBeef" />

    <Button
        android:id="@+id/btnChkn"
        android:layout_width="180dp"
        android:layout_height="83dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        android:background="#F3DFDD"
        android:text="Chkn"
        android:textSize="30sp"
        app:layout_constraintStart_toEndOf="@+id/btnSaus"
        app:layout_constraintTop_toBottomOf="@+id/btnHam" />

    <Button
        android:id="@+id/btnCheesestick"
        android:layout_width="180dp"
        android:layout_height="83dp"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:background="#F3DFDD"
        android:text="ISaus"
        android:textSize="30sp"
        app:layout_constraintEnd_toStartOf="@+id/btnChzo"
        app:layout_constraintStart_toEndOf="@+id/btnPan"
        app:layout_constraintTop_toBottomOf="@+id/btnSaus" />

    <Button
        android:id="@+id/btnChzo"
        android:layout_width="180dp"
        android:layout_height="83dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        android:background="#F3DFDD"
        android:text="Chzo"
        android:textSize="30sp"
        app:layout_constraintStart_toEndOf="@+id/btnCheesestick"
        app:layout_constraintTop_toBottomOf="@+id/btnChkn" />

    <Button
        android:id="@+id/btnMush"
        android:layout_width="118dp"
        android:layout_height="114dp"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="8dp"
        android:background="#CBF3CD"
        android:text="Mush"
        android:textSize="30sp"
        app:layout_constraintStart_toEndOf="@+id/btnBacon"
        app:layout_constraintTop_toBottomOf="@+id/textView9" />

    <Button
        android:id="@+id/btnGrp"
        android:layout_width="118dp"
        android:layout_height="114dp"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="8dp"
        android:background="#CBF3CD"
        android:text="Grp"
        android:textSize="30sp"
        app:layout_constraintStart_toEndOf="@+id/btnHam"
        app:layout_constraintTop_toBottomOf="@+id/btnMush" />

    <Button
        android:id="@+id/btnOn"
        android:layout_width="118dp"
        android:layout_height="114dp"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="8dp"
        android:background="#CBF3CD"
        android:text="On"
        android:textSize="30sp"
        app:layout_constraintStart_toEndOf="@+id/btnChzo"
        app:layout_constraintTop_toBottomOf="@+id/btnGrp" />

    <Button
        android:id="@+id/btnTom"
        android:layout_width="118dp"
        android:layout_height="114dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        android:background="#CBF3CD"
        android:text="Tom"
        android:textSize="30sp"
        app:layout_constraintStart_toEndOf="@+id/btnMush"
        app:layout_constraintTop_toBottomOf="@+id/textView9" />

    <Button
        android:id="@+id/btnBo"
        android:layout_width="118dp"
        android:layout_height="114dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        android:background="#CBF3CD"
        android:text="Bo"
        android:textSize="30sp"
        app:layout_constraintStart_toEndOf="@+id/btnGrp"
        app:layout_constraintTop_toBottomOf="@+id/btnTom" />

    <Button
        android:id="@+id/btnGo"
        android:layout_width="118dp"
        android:layout_height="114dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        android:background="#CBF3CD"
        android:text="Go"
        android:textSize="30sp"
        app:layout_constraintStart_toEndOf="@+id/btnOn"
        app:layout_constraintTop_toBottomOf="@+id/btnBo" />

    <Button
        android:id="@+id/btnPine"
        android:layout_width="118dp"
        android:layout_height="114dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        android:background="#CBF3CD"
        android:text="Pine"
        android:textSize="30sp"
        app:layout_constraintStart_toEndOf="@+id/btnTom"
        app:layout_constraintTop_toBottomOf="@+id/textView9" />

    <Button
        android:id="@+id/btnJal"
        android:layout_width="118dp"
        android:layout_height="114dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        android:background="#CBF3CD"
        android:text="Jal"
        android:textSize="30sp"
        app:layout_constraintStart_toEndOf="@+id/btnBo"
        app:layout_constraintTop_toBottomOf="@+id/btnPine" />

    <Button
        android:id="@+id/btnBp"
        android:layout_width="118dp"
        android:layout_height="114dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        android:background="#CBF3CD"
        android:text="Bp"
        android:textSize="30sp"
        app:layout_constraintStart_toEndOf="@+id/btnGo"
        app:layout_constraintTop_toBottomOf="@+id/btnJal" />

    <Button
        android:id="@+id/btnSend"
        android:layout_width="244dp"
        android:layout_height="81dp"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="16dp"
        android:background="#055DA0"
        android:text="SEND"
        android:textSize="42sp"
        app:layout_constraintStart_toEndOf="@+id/spinnerSauce"
        app:layout_constraintTop_toBottomOf="@+id/btnBp" />

    <TextView
        android:id="@+id/txtPizzaString"
        android:layout_width="340dp"
        android:layout_height="72dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="16dp"
        android:gravity="center"
        android:text="Regular,Pep,Bacon,Mush,Pine,Chkn,Saus"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnPan" />

    <Spinner
        android:id="@+id/spinnerCheese"
        android:layout_width="150dp"
        android:layout_height="81dp"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="16dp"
        android:entries="@array/cheeses"
        android:text="Cheese"
        app:layout_constraintStart_toEndOf="@+id/txtPizzaString"
        app:layout_constraintTop_toBottomOf="@+id/btnChzo" />

    <Spinner
        android:id="@+id/spinnerSauce"
        android:layout_width="150dp"
        android:layout_height="81dp"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="16dp"
        android:entries="@array/sauces"
        app:layout_constraintStart_toEndOf="@+id/spinnerCheese"
        app:layout_constraintTop_toBottomOf="@+id/btnOn" />

</android.support.constraint.ConstraintLayout>